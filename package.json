{
  "name": "nasjonalturbase",
  "version": "1.0.0",
  "private": true,
  "description": "API for Nasjonal Turbase",
  "homepage": "http://www.nasjonalturbase.no/",
  "bugs": "https://github.com/turistforeningen/nasjonalturbase/issues",
  "repository": "git@github.com:Turistforeningen/nasjonalturbase.git",
  "main": "src/server.js",
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@turistforeningen.no>",
  "dependencies": {
    "JSONStream": "0.10.0",
    "coffee-script": "1.8.0",
    "express": "3.20.2",
    "joi": "5.0.0",
    "mongo-querystring": "0.0.2",
    "mongodb": "2.0.28",
    "raven": "0.7.3",
    "redis": "0.12.1",
    "supervisor": "0.6.0"
  },
  "devDependencies": {
    "async": "~0",
    "coffeelint": "~1",
    "concat-stream": "~1",
    "mocha": "~2",
    "supertest": "~0"
  },
  "scripts": {
    "start": "supervisor -w src -- src/server.js",
    "devserver": "supervisor -w coffee -- coffee/server.litcoffee",
    "test": "mocha -b -c --check-leaks test/suite.coffee -R spec --compilers coffee:coffee-script/register",
    "watch": "mocha -b -c --check-leaks test/suite.coffee -R progress --compilers coffee:coffee-script/register --watch",
    "hint": "coffeelint coffee/ test/",
    "postinstall": "rm -rf src/* && coffee --bare -o src/ -c coffee/"
  },
  "config": {
    "port_www": 8080,
    "sentry_dns": null,
    "mongo_uri": null,
    "dotcloud_cache_redis_port": null,
    "dotcloud_cache_redis_host": null,
    "dotcloud_cache_redis_password": null
  },
  "license": "MIT"
}
