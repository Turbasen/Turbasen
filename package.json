{
  "name": "nasjonalturbase",
  "version": "1.0.0",
  "private": true,
  "description": "API for Nasjonal Turbase",
  "homepage": "http://www.nasjonalturbase.no/",
  "bugs": "https://github.com/turistforeningen/nasjonalturbase/issues",
  "repository": "git@github.com:Turistforeningen/nasjonalturbase.git",
  "main": "src/server.js",
  "author": "Hans Kristian Flaatten <hans.kristian.flaatten@turistforeningen.no>",
  "dependencies": {
    "coffee-script": "~1",
    "mongodb": "~2.0",
    "mongo-querystring": "~0",
    "express": "~3",
    "raven": "~0.6",
    "redis": "~0",
    "JSONStream": "~0",
    "joi": "~4"
  },
  "devDependencies": {
    "async": "~0",
    "concat-stream": "~1",
    "mocha": "~2",
    "supertest": "~0",
    "supervisor": "~0"
  },
  "scripts": {
    "start": "supervisor -w coffee -- coffee/server.litcoffee",
    "test": "SILENT=ture mocha -b -c --check-leaks test/suite.coffee -R progress --compilers coffee:coffee-script/register -w",
    "postinstall": "rm -rf src/* && coffee --bare -o src/ -c coffee/; if [ -d .git/hooks/ ]; then cp config/git-hooks/pre-commit .git/hooks/.; fi",
    "test-drone": "SILENT=true node_modules/.bin/mocha test/suite.coffee -R spec --compilers coffee:coffee-script/register"
  },
  "config": {
    "port_www": 8080,
    "sentry_dns": null,
    "mongo_uri": null,
    "dotcloud_cache_redis_port": null,
    "dotcloud_cache_redis_host": null,
    "dotcloud_cache_redis_password": null
  },
  "license": "MIT"
}
